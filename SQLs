CREATE TABLE `capitalonedb`.`co_clientdetails` ( `CLIENTID` VARCHAR(10) NOT NULL , `PAN` VARCHAR(10) NOT NULL , `PHONE` VARCHAR(10) NOT NULL , `NAME` VARCHAR(100) NOT NULL ) ENGINE = InnoDB; 
ALTER TABLE `co_clientdetails` ADD PRIMARY KEY(`CLIENTID`); 

CREATE TABLE `capitalonedb`.`co_login` ( `CLIENTID` VARCHAR(10) NOT NULL , `PASSWORD` VARCHAR(200) NOT NULL) ENGINE = InnoDB; 
ALTER TABLE `co_login` ADD CONSTRAINT fk_clientid FOREIGN KEY(`CLIENTID`) REFERENCES co_clientdetails(CLIENTID); 
